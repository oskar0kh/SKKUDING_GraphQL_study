meta {
  name: 특정 name restaurant 가져오기
  type: graphql
  seq: 2
}

post {
  url: http://localhost:3000/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  query {
    getRestaurantByName(search_name: "성대 밥상") {
      name
      address
      phone
      rating
    }
  }
}

settings {
  encodeUrl: true
}
